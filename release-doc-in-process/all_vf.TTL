        rdfs:comment        "Relates a process input or output (or other vf:EconomicEVent) to a verb, such as consume, produce, work, improve, etc." .

vf:committedAction
        a                   owl:ObjectProperty ;
        rdfs:domain         vf:Commitment ;
        rdfs:range          vf:Action ;
        rdfs:comment        "Relates a process input or output (or other vf:EconomicEVent) to a verb, such as consume, produce, work, improve, etc." .

