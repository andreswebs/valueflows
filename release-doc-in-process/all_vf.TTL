# ABSTRACT CLASSES

vf:Action  a           owl:Class ;
        rdfs:label           "vf:Action" ;
        rdfs:subClassOf   rdf:Property ;
        rdfs:comment   "An action verb defining the kind of event, commitment, or intent." .

